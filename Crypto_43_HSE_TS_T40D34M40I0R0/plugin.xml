<plugin>
  <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="Crypto_43_HSE_TS_T40D34M40I0R0_ModuleId"
             name="Crypto_43_HSE_TS_T40D34M40I0R0 Module">

    <module id="Crypto_43_HSE_TS_T40D34M40I0R0"
            label="Crypto_43_HSE"
            mandatory="false"
            allowMultiple="false"
            description="Crypto_43_HSE BSW module"
            copyright="Copyright 2020 - 2024 NXP"
            swVersionMajor="4"
            swVersionMinor="0"
            swVersionPatch="0"
            swVersionSuffix="P16_D2403"
            specVersionMajor="4"
            specVersionMinor="7"
            specVersionPatch="0"
            specVersionSuffix="Rev_0000"
            relVersionPrefix="AUTOSAR"
            relVersionMajor="4"
            relVersionMinor="7"
            relVersionPatch="0"
            relVersionSuffix="0000"
            categoryType="Crypto_43_HSE"
            categoryLayer="MCAL"
            categoryCategory="ECU Firmware"
            categoryComponent="ECUC">
      <ecuType target="CORTEXM" derivate="S32K3XX"/>
      <ecuType target="CORTEXM" derivate="S32M27X"/>

    </module>
    </extension>
   
  <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="Crypto_43_HSE_TS_T40D34M40I0R0_ConfigId"
             name="Crypto_43_HSE_TS_T40D34M40I0R0 Configuration">
    <configuration moduleId="Crypto_43_HSE_TS_T40D34M40I0R0">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
          <!-- register the main xdm configuration schema for Crypto_43_HSE -->
          <resource value="config/Crypto_43_HSE.xdm" type="xdm"/>

          <!-- register the recommended configuration no. 1 for Crypto_43_HSE -->
          <resource value="config_ext/Crypto_43_HSERecConfiguration_SupportedPrimitives_Standard.xdm" 
                    id="res_Crypto_43_HSERecConfiguration_SupportedPrimitives_Standard" 
                    type="xdm"/>
          <recconfig name="Crypto_43_HSERecConfiguration_SupportedPrimitives_Standard" 
                     default="false" 
                     description="Supported primitives in Crypto driver for S32K3 platform using Standard HSE FW"
                     resourceId="res_Crypto_43_HSERecConfiguration_SupportedPrimitives_Standard" 
                     path="ASPath:/Crypto_43_HSE/Crypto"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/Crypto_43_HSE_TS_T40D34M40I0R0/Crypto"/>
      </data>

      <editor id="Crypto_43_HSE_TS_T40D34M40I0R0_EditorId"
              label="Default"
              tooltip="Crypto_43_HSE BSW module">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/Crypto_43_HSE_TS_T40D34M40I0R0/Crypto"/>
          <parameter name="title" value="Crypto_43_HSE"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>

 <extension point="dreisoft.tresos.launcher2.plugin.generator"
             id="EPCGenerator"
             name="EPC Generator">
  <generator moduleId="Crypto_43_HSE_TS_T40D34M40I0R0"
               id="Crypto_43_HSE_TS_T40D34M40I0R0"
               class="dreisoft.tresos.autosar2.generator.EPCFileGenerator">
   
  <parameter name="allVariants" value="false"/>
  <parameter name="cfgFilePath" value="output"/>
  <parameter name="generateAllModules" value="false"/>
  <parameter name="generateIntoOneFile" value="false"/>
  <parameter name="contentType" value="asc:4.7.0"/>
  </generator>
 </extension>
  
 <extension point="dreisoft.tresos.generator.api.plugin.generator" id="Crypto_43_HSE_TS_T40D34M40I0R0_NGGeneratorId">
          <generator moduleId="Crypto_43_HSE_TS_T40D34M40I0R0"
                     class="dreisoft.tresos.generator.ng.api.NGGenerator"
                     id="Crypto_43_HSE_TS_T40D34M40I0R0_UniqueNGGeneratorId"
                     modes="generate_PC,verify"
                     step="post"> <!-- run after code-generation -->
           <parameter name="variantAware" value="false"/>
          </generator>
  </extension>
      <extension point="dreisoft.tresos.launcher2.plugin.generator"
              id="Crypto_43_HSE_TS_T40D34M40I0R0_GeneratorId"
              name="Crypto_43_HSE_TS_T40D34M40I0R0 Generator">
    <generator moduleId="Crypto_43_HSE_TS_T40D34M40I0R0"
               class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator">
  
    <parameter name="variantAware" value="false"/>
    <!-- swcd modes and template path parameters -->
    <parameter name="mode_type" mode="generate_swcd" value="generate"></parameter>
    <parameter name="mode_type" mode="verify_swcd" value="verify"></parameter>
    <parameter name="templates" mode="generate_swcd,verify_swcd" value="generate_swcd"></parameter>   

      <!-- common template path parameters -->
      <parameter name="templates" mode="generate,verify" value="generate,generate_PC"/>
      <!-- custom generators -->
      <parameter name="templates" mode="generate_PC" value="generate_PC"/>
      <!-- custom template paths -->
      <parameter name="mode_type" mode="generate_PC" value="generate"/>
    </generator>
  </extension>
  
  <extension point="org.eclipse.help.toc">
    <toc file="anchors.xml" primary="true"/>
    <toc file="$nl$/Crypto_43_HSE/toc.xml" primary="false"/>
    <!-- toc file="$nl$/ReleaseNotes/toc.xml" primary="false"/ !-->
  </extension>
</plugin>
